{
	"name": "ms-markets",
	"version": "2.0.0",
	"description": "Crypolio markets microservice.",
	"author": "Choleski Louis <choleski@gmx.com> (https://www.leadeasygen.com)",
	"main": "dist/index.js",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/leadeasygen/api.git"
	},
	"scripts": {
		"build": "rimraf ./dist && tsc",
		"start": "npm run build && node dist/index.js",
		"start:dev": "nodemon",
		"lint": "eslint ./lib --ext .ts",
		"lint:fix": "eslint ./lib --ext .ts,.tsx --fix",
		"format": "prettier --write ./lib",
		"prettier-format": "run-script-os",
		"prettier-format:win32": "prettier --config .prettierrc \"./lib/**/*.ts\" --write",
		"prettier-format:darwin:linux": "prettier --config .prettierrc 'lib/**/*.ts' --write",
		"prettier-format:default": "prettier --config .prettierrc 'lib/**/*.ts' --write",
		"prettier-watch": "run-script-os",
		"prettier-watch:win32": "onchange \"lib/**/*.ts\" -- prettier --write {{changed}}",
		"prettier-watch:darwin:linux": "onchange 'lib/**/*.ts' -- prettier --write {{changed}}",
		"prettier-watch:default": "onchange 'lib/**/*.ts' -- prettier --write {{changed}}",
		"test": "jest",
		"test:dev": "jest --watchAll",
		"setup": "NODE_ENV=init node --inspect=0.0.0.0:7002 -r ts-node/register lib/utils/init.js"
	},
	"husky": {
		"hooks": {
			"pre-commit": "npm run test && npm run prettier-format && npm run lint"
		}
	},
	"keywords": [],
	"license": "ISC",
	"private": true,
	"dependencies": {
		"@koa/cors": "^4.0.0",
		"asciichart": "^1.5.25",
		"axios": "^1.7.7",
		"dotenv-safe": "^8.2.0",
		"eslint-plugin-jest": "^27.6.0",
		"fs": "^0.0.1-security",
		"jsonwebtoken": "^9.0.2",
		"koa": "^2.14.2",
		"koa-bodyparser": "^4.4.1",
		"koa-compress": "^5.1.1",
		"koa-helmet": "^6.1.0",
		"koa-logger": "^3.2.1",
		"koa-router": "^12.0.1",
		"log-update": "^5.0.1",
		"moment": "^2.29.4",
		"os": "^0.1.2",
		"path": "^0.12.7",
		"pg": "^8.11.3",
		"playwright": "^1.48.0",
		"ts-jest": "^29.1.1"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.23.5",
		"@types/jest": "^29.5.13",
		"@types/json5": "^2.2.0",
		"@types/jsonwebtoken": "^9.0.5",
		"@types/koa": "^2.13.12",
		"@types/koa-bodyparser": "^4.3.12",
		"@types/koa-compress": "^4.0.6",
		"@types/koa-helmet": "^6.0.8",
		"@types/koa-router": "^7.4.8",
		"@types/koa__cors": "^3.3.1",
		"@types/node": "^18.19.54",
		"@typescript-eslint/eslint-plugin": "^7.18.0",
		"@typescript-eslint/parser": "^7.18.0",
		"babel-jest": "^29.7.0",
		"core-js": "^3.33.3",
		"eslint": "^8.57.1",
		"eslint-config-airbnb-typescript": "^18.0.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-import-resolver-typescript": "^3.6.3",
		"eslint-plugin-import": "^2.30.0",
		"eslint-plugin-prettier": "5.0.0",
		"husky": "^8.0.3",
		"jest": "^29.7.0",
		"nodemon": "^3.0.1",
		"onchange": "^7.1.0",
		"prettier": "^3.3.3",
		"rimraf": "^3.0.2",
		"run-script-os": "^1.1.6",
		"supertest": "^6.3.3",
		"ts-node": "^10.9.1",
		"typescript": "^4.9.5"
	}
}
